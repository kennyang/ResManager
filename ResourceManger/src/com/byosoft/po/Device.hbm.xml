<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.byosoft.po.Device">
		<id name="Id" column="Id">
			<generator class="increment"></generator>
		</id>
		<property name="devicename" column="devicename"></property>
		<property name="ower" column="ower"></property>
		<property name="user" column="user"></property>
		<property name="content" column="content"></property>
		<property name="modifytime" column="modifytime"></property>
	</class>
</hibernate-mapping>